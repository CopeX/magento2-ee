{
  "name": "wirecard/magento2-ee",
  "description": "Magento2 plugin for Elastic Engine",
  "license": "GPL-3.0",
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  ],
  "require": {
    "php": ">=5.6",
    "magento/module-payment": "*",
    "roave/security-advisories": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "type": "magento2-module",
  "scripts": {
    "test": "phpunit --colors=always"
  },
  "autoload": {
    "psr-4": {
      "Wirecard\\ElasticEngine\\": "src"
    },
    "files": [
      "registration.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "WirecardTest\\ElasticEngine\\": "test"
    }
  }
}
